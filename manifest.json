{
    "name": "User Sessions Tracker",
    "description": "Сбор действий пользователя и создание сессий",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png",
        "default_title": "Открыть меню"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}